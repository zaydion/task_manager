<nav>
  <% if current_user %>
    <ul>
      <li><%= link_to current_user.username, current_user %></li>
      <li><%= link_to 'Sign Out', signout_path %></li>
    </ul>
  <% else %>
    <ul>
      <li><%= link_to 'Sign In', signin_path %></li>
      <li><%= link_to 'Sign Up', new_user_path %></li>
    </ul>
  <% end %>
</nav>
